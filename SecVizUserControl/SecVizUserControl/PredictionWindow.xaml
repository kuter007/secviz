<Window x:Class="SecVizAdminApp.PredictionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PredictionWindow" Height="304" Width="683">
    <Grid>
        <DataGrid AutoGenerateColumns="True" ItemsSource="{Binding HyperAlerts}" Height="8" HorizontalAlignment="Left" Margin="12,12,0,0" Name="dataGrid" VerticalAlignment="Top" Width="7" >
            
        </DataGrid>
        <Button Content="View Details" Height="23" HorizontalAlignment="Left" Margin="41,216,0,0" Name="findButton" VerticalAlignment="Top" Width="126" Click="findButton_Click" />
        <Button Content="Close" Height="23" HorizontalAlignment="Left" Margin="211,216,0,0" Name="button1" VerticalAlignment="Top" Width="108" Click="button1_Click" />
        <ListView Height="179" HorizontalAlignment="Left" Margin="12,12,0,0" Name="alertnameListview" VerticalAlignment="Top" Width="352" ItemsSource="{Binding HyperAlerts}" />
        <TextBlock Height="227" HorizontalAlignment="Left" Margin="408,12,0,0" Name="detailBlock" Text="TextBlock" VerticalAlignment="Top" Width="241" />
    </Grid>
</Window>
